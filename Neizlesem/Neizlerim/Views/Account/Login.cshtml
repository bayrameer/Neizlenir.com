
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">

<!-- Mirrored from storage.googleapis.com/theme-vessel-items/checking-sites/oddo-2-html/HTML/main/login-7.html by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 01 Apr 2022 05:48:26 GMT -->
<head>
    <title>Login</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta charset="UTF-8">
    <!-- External CSS libraries -->
    <link type="text/css" rel="stylesheet" href="/admin/assets/css/bootstrap.min.css">
    <link type="text/css" rel="stylesheet" href="/admin/assets/fonts/font-awesome/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" href="/admin/assets/fonts/flaticon/font/flaticon.css">

    <!-- Favicon icon -->
    <link rel="shortcut icon" href="assets/img/favicon.ico" type="image/x-icon">

    <!-- Google fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Jost:wght@300;400;500;600;700;800;900&amp;display=swap" rel="stylesheet">

    <!-- Custom Stylesheet -->
    <link type="text/css" rel="stylesheet" href="/admin/assets/css/style.css">

</head>
<body id="top">
    <div class="page_loader"></div>

    <!-- Login 7 start -->
    <div class="login-7">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="form-section">
                        <div class="logo">
                            <a>
                                <img src="/img/logo/logo.png" height="20" alt="logo">
                            </a>
                        </div>
                        <h3>Giriş Yap</h3>
                        <div class="login-inner-form">
                            <form method="post" asp-controller="Account" asp-action="Login">
                                <div class="form-group clearfix">
                                    <div class="form-box">
                                        <input name="userName" type="text" id="username" class="form-control" placeholder="Kullanıcı Adı" aria-label="Kullanıcı Adı">
                                        <i class="flaticon-mail-2"></i>
                                    </div>
                                </div>
                                <div class="form-group clearfix">
                                    <div class="form-box">
                                        <input name="password" type="password" id="pass" class="form-control" autocomplete="off" placeholder="Şifre" aria-label="Password">
                                        <i class="flaticon-password"></i>
                                    </div>
                                </div>
                                <div class="checkbox form-group clearfix">
                                    <div class="form-check float-start">
                                        <input class="form-check-input" type="checkbox" id="rememberme">
                                        <label class="form-check-label" for="rememberme">
                                            Beni Hatırla
                                        </label>
                                    </div>
                                    <a href="forgot-password-7.html" class="link-light float-end forgot-password">Şifremi Unuttum?</a>
                                </div>
                                <div class="form-group clearfix">
                                    <button type="submit" class="btn btn-primary btn-lg btn-theme">Giriş Yap</button>
                                </div>
                            </form>

                        </div>
                        <!--  <<p>Üye Değil Misiniz?<a asp-controller="Account" asp-action="Register"> Üye Ol</a></p>-->
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Login 7 end -->
    <!-- External JS libraries -->
    <script src="/admin/assets/js/jquery-3.6.0.min.js"></script>
    <script src="/admin/assets/js/bootstrap.bundle.min.js"></script>
    <script src="/admin/assets/js/jquery.validate.min.js"></script>
    <script src="/admin/assets/js/app.js"></script>
    <!-- Custom JS Script -->
</body>

